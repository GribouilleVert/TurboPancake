{
    "name": "vasco/turbopancake",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Vasco Compain",
            "email": "vasco.gribouille@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "TurboPancake\\": "src/TurboPancake",
            "TurboModule\\": "src"
        },
        "files": [
            "src/TurboPancake/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3",

        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-intl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-iconv": "*",

        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^0.4.1",
        "kriswallsmith/buzz": "^1.1",
        "http-interop/response-sender": "^1.0.0",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "twig/twig": "^3.0.0",
        "php-di/php-di": "^6.0.0",
        "pagerfanta/pagerfanta": "^2.1.0",
        "psr/http-server-middleware": "^1.0.0",
        "middlewares/whoops": "^1.0",
        "intervention/image": "^2.5.0",
        "phpmailer/phpmailer": "^6.0.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0@dev",
        "robmorgan/phinx": "^0.10.0",
        "fzaninotto/faker": "^1.9@dev",
        "symfony/var-dumper": "^5.0@dev",
        "phpunit/phpunit": "^8.1.0"
    }
}
